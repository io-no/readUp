<resources xmlns:tools="http://schemas.android.com/tools">


    <style name="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">#FFFFFF</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="windowActionBar">false</item>
    </style>
    <style name="Theme.MaterialComponents.Light.NoActionBar.False">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
        <item name="colorPrimary">#FFFFFF</item>
        <item name="android:textColorPrimary">@color/colorPrimary</item>
    </style>
    <style name="Theme.MaterialComponents.Light.NoActionBar.G">
        <item name="colorControlHighlight">#FFFFFF</item>
    </style>

    <style name="Theme.MaterialComponents.Light.NoActionBar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

<style name="Theme.AppCompat.Light.Dialog.Alert">
    <item name="android:textColorPrimary">@android:color/tertiary_text_light</item>
</style>



</resources>
